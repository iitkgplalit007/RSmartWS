  <step>
    <name>NameAndAddress</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>CC</connection>
    <sql>select 
link.LINKID,
link.CARRIER_CODE,
link.CLAIM_NUMBER,
link.POLICY_NUMBER_IDENTIFIER,
link.POLICY_EFFECTIVE_DATE_ITEM,
link.SEPARATED_SEGMENT_NUMBER,
link.EXPOSURE_STATE_CODE,
link.REPORT_LEVEL_CODE_REPORT_NUM_0,
link.CORRECTION_SEQUENCE_NUMBER_C_0,
NamenAddress.InsuredName,
NamenAddress.INSUREDADDRESS
 from 
(SELECT
CLAIM.ClaimNumber as CLAIMNUMBER,
CLAIM.ID as ID,
POLICY.PolicyNumber as POLICY_NUMBER_IDENTIFIER,
POLICY.EffectiveDate as POLICY_EFFECTIVE_DATE_ITEM,
CASE WHEN CONTACT.NAME IS NULL THEN CONCAT (CONTACT.FirstName, ' ' , CONTACT.LastName) ELSE CONTACT.NAME END AS INSUREDNAME, 
CONCAT (CONTACTADDRESS.ADDRESSLINE1 , ' ' , CONTACTADDRESS.ADDRESSLINE2  , ' ' ,
 CONTACTADDRESS.ADDRESSLINE3  , ' ' , CONTACTADDRESS.CITY  , ' ' ,
 CONTACTADDRESS.STATE , ' ' ,CONTACTADDRESS.POSTALCODE) AS INSUREDADDRESS
FROM CC_CLAIM CLAIM
JOIN CC_POLICY POLICY ON POLICY.ID = CLAIM.POLICYID
JOIN CC_CLAIMCONTACT CLAIMCONTACT ON CLAIMCONTACT.CLAIMID = CLAIM.ID
JOIN CC_CLAIMCONTACTROLE CLAIMCONTACTROLE ON CLAIMCONTACTROLE.CLAIMCONTACTID= CLAIMCONTACT.ID
JOIN CC_CONTACT CONTACT ON CLAIMCONTACT.CONTACTID =  CONTACT.ID
JOIN CC_ADDRESS CONTACTADDRESS ON CONTACT.PRIMARYADDRESSID = CONTACTADDRESS.ID
JOIN CCTL_CONTACTROLE ROLETYPE ON ROLETYPE.ID = CLAIMCONTACTROLE.ROLE 
JOIN CCTL_CONTACT CONTACTSUBTYPE ON CONTACT.SUBTYPE = CONTACTSUBTYPE.ID
WHERE ROLETYPE.NAME = 'INSURED') NamenAddress
join 
(Select 
CLAIM.ID as LINKID,
CLAIM.ClaimNumber as CLAIM_NUMBER,
'XXX01' as CARRIER_CODE,
POLICY.EffectiveDate as POLICY_EFFECTIVE_DATE_ITEM,
POLICY.PolicyNumber as POLICY_NUMBER_IDENTIFIER,
'001' as SEPARATED_SEGMENT_NUMBER,
'04' as EXPOSURE_STATE_CODE,
'1' as REPORT_LEVEL_CODE_REPORT_NUM_0,
'0' as CORRECTION_SEQUENCE_NUMBER_C_0
FROM cc_claim CLAIM 
JOIN CC_POLICY POLICY ON policy.ID = CLAIM.PolicyID) link
on link.CLAIM_NUMBER = NamenAddress.CLAIMNUMBER and link.LINKID = NamenAddress.ID
and link.POLICY_EFFECTIVE_DATE_ITEM = NamenAddress.POLICY_EFFECTIVE_DATE_ITEM
and link.POLICY_NUMBER_IDENTIFIER = NamenAddress.POLICY_NUMBER_IDENTIFIER
</sql>
