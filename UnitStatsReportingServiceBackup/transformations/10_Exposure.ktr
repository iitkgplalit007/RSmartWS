    <connection>PC</connection>
    <sql>select 
period.PolicyNumber AS POLICY_NUMBER,
classcode.Code as CLASSIFICATION_CODE,
modifier.EffectiveDate as EXPERIENCE_MODIFICATION_EFFE_0,
rateFactor.Assessment as EXPERIENCE_MODIFICATION_FACT_0,
coveredemployee.BasisAmount as EXPOSURE_AMOUNT,
period.TotalPremiumRPT as PREMIUM_AMOUNT,
'0' as SPLIT_PERIOD_CODE,
'A' as UPDATE_TYPE_CODE,
'00' as EXPOSURE_ACT_EXPOSURE_COVERA_0
from PC9_local.dbo.pc_wccost cost 
left join PC9_local.dbo.pc_wctransaction transactions on  transactions.WCCost = cost.ID
left join PC9_local.dbo.pc_policyperiod period on period.ID = transactions.BranchID
left join PC9_local.dbo.pc_wcmodifier modifier on period.ID = modifier.BranchID
left join PC9_local.dbo.pc_wcratefactor rateFactor on modifier.ID = rateFactor.WCModifier
left join PC9_local.dbo.pc_wcjurisdiction jurisdiction on jurisdiction.ID = modifier.WCJurisdiction
left join PC9_local.dbo.pc_wccoveredemployee coveredemployee on coveredemployee.BranchID = period.ID
left join PC9_local.dbo.pc_wcclasscode classcode on classcode.ID = coveredemployee.ClassCodeID
left join PC9_local.dbo.pctl_specialcov governingLaw on coveredemployee.SpecialCov = governingLaw.ID
--where period.PolicyNumber ='53-263535'
where period.PolicyNumber is not null
--and modifier.EffectiveDate is not null;</sql>
