    <connection>CC</connection>
    <sql>Select 
LTRIM(RTRIM(POLICY.PolicyNumber)) as POLICY_NUMBER,
CLAIM.CLAIMNUMBER,
CLAIM.ID as ID,
'XXX01' as CARRIER_CODE,
'001' as SEPARATED_SEGMENT_NUMBER,
'04' as EXPOSURE_STATE_CODE,
POLICY.EffectiveDate as POLICY_EFFECTIVE_DATE_ITEM,
'1' as REPORT_LEVEL_CODE_REPORT_NUM_0,
'0' as CORRECTION_SEQUENCE_NUMBER_C_0
FROM  CC9_LOCAL.dbo.cc_claim CLAIM 
JOIN CC9_LOCAL.dbo.CC_POLICY POLICY ON policy.ID = CLAIM.PolicyID
where policy.PolicyNumber in ('53-263535', '23-502011', '53-235675', '4775949865-02', '23-502011', '53-235675')
--where policy.PolicyNumber in(select distinct(policy.PolicyNumber) from cc_policy policy)</sql>
