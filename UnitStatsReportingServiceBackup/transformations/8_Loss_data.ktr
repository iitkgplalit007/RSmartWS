loss.ACCIDENT_DATE_ITEM,
loss.CARRIER_CODE,
loss.CAUSE_OF_INJURY_CODE,
loss.CLAIM_NUMBER,
loss.CLAIM_STATUS_CODE,
loss.CLASSIFICATION_CODE,
loss.CORRECTION_SEQUENCE_NUMBER_C_0,
loss.EXPOSURE_STATE_CODE,
loss.FRAUDULENT_CLAIM_CODE,
loss.INCURRED_INDEMNITY_AMOUNT,
loss.INCURRED_MEDICAL_AMOUNT,
loss.INJURY_CODE,
loss.JURISDICTION_STATE_CODE,
loss.LOSS_COVERAGE_ACT_CODE,
loss.NATURE_OF_INJURY_CODE,
loss.OCCUPATION_DESCRIPTION,
loss.PAID_ALLOCATED_LOSS_ADJUSTME_1,
loss.PAID_INDEMNITY_AMOUNT,
loss.PAID_MEDICAL_AMOUNT,
loss.PART_OF_BODY_CODE,
loss.POLICY_EFFECTIVE_DATE_ITEM,
loss.POLICY_NUMBER_IDENTIFIER,
loss.REPORT_LEVEL_CODE_REPORT_NUM_0,
loss.SCHEDULED_INDEMNITY_PERCENTA_0,
loss.SEPARATED_SEGMENT_NUMBER,
loss.TOTAL_GROSS_INCURRED_AMOUNT,
loss.TOTAL_INCURRED_VOCATIONAL_RE_0,
loss.TYPE_OF_CLAIM_CODE,
loss.TYPE_OF_LOSS_CODE,
loss.TYPE_OF_RECOVERY_CODE,
loss.TYPE_OF_SETTLEMENT_CODE,
loss.UPDATE_TYPE_CODE,
loss.VOCATIONAL_REHABILITATION_EV_0,
loss.WEEKLY_WAGE_AMOUNT,
link.LINKID
 from
(SELECT
CLASSCODE.CODE AS CLASSIFICATION_CODE, 
CLAIM.LossDate AS ACCIDENT_DATE_ITEM,
CASE
     WHEN CLAIM.STATE = 3 THEN 1
     WHEN CLAIM.STATE = 5 THEN 1
	 ELSE 0 END AS CLAIM_STATUS_CODE,
EMPLOYMENTDATA.WageAmount AS WEEKLY_WAGE_AMOUNT,
CASE
 WHEN INCIDENT.SEVERITY = 10017 THEN '01'
 WHEN INCIDENT.SEVERITY = 10010 THEN '02'
 WHEN INCIDENT.SEVERITY = 10007 THEN '03'
 WHEN INCIDENT.SEVERITY = 10001 THEN '04'
 WHEN INCIDENT.SEVERITY = 10012 THEN '05'
 WHEN INCIDENT.SEVERITY = 10008 THEN '06'
 WHEN INCIDENT.SEVERITY = 10018 THEN '07'
 WHEN INCIDENT.SEVERITY = 10011 THEN '08'
 WHEN INCIDENT.SEVERITY = 10013 THEN '09'
 ELSE '08'
 END AS INJURY_CODE,
 CASE
 WHEN EXPOSURE.EXPOSURETYPE = 6 THEN (exposureReport.OpenReservesRprtng + exposureReport.TotalPaymentsRprtng)-exposureReport.TotalRecoveriesRprtng
 ELSE 0 END AS INCURRED_INDEMNITY_AMOUNT,
 CASE 
 WHEN EXPOSURE.EXPOSURETYPE = 7 THEN (exposureReport.OpenReservesRprtng + exposureReport.TotalPaymentsRprtng)-exposureReport.TotalRecoveriesRprtng
 ELSE 0 END AS INCURRED_MEDICAL_AMOUNT,
 'A' AS UPDATE_TYPE_CODE,
 '00' AS LOSS_COVERAGE_ACT_CODE,
CASE
WHEN INCIDENT.SUBTYPE = 5 THEN 
  CASE WHEN INCIDENT.GeneralInjuryType = 10001 THEN '01'
       WHEN INCIDENT.GeneralInjuryType = 10002 THEN '03'
	   WHEN INCIDENT.GeneralInjuryType = 10003 THEN '02'
	   ELSE ' ' END
ELSE ' ' END AS TYPE_OF_LOSS_CODE,
CASE
WHEN transactions.Subtype = 3 or transactions.Subtype = 4 THEN
     CASE WHEN transactions.RecoveryCategory = 10005 THEN '03'
	 ELSE '01' END
ELSE '01' END AS TYPE_OF_RECOVERY_CODE,
'01' as TYPE_OF_CLAIM_CODE,
'02' AS TYPE_OF_SETTLEMENT_CODE,
12000.00 as TOTAL_INCURRED_VOCATIONAL_RE_0,
jurisdiction.TYPECODE As JURISDICTION_STATE_CODE,
'10' as PART_OF_BODY_CODE,
'01' as NATURE_OF_INJURY_CODE,
25 as CAUSE_OF_INJURY_CODE,
'N' as VOCATIONAL_REHABILITATION_EV_0,
'00' as FRAUDULENT_CLAIM_CODE,
CASE WHEN CONTACTROLE.ROLE = 2 THEN CONTACT.OCCUPATION ELSE '' END AS OCCUPATION_DESCRIPTION,
CASE
 WHEN EXPOSURE.EXPOSURETYPE = 6 THEN exposureReport.TotalPaymentsRprtng
 ELSE 0 END AS PAID_INDEMNITY_AMOUNT,
 CASE 
 WHEN EXPOSURE.EXPOSURETYPE = 7 THEN exposureReport.TotalPaymentsRprtng
 ELSE 0 END AS PAID_MEDICAL_AMOUNT,
 13000.00 as TOTAL_GROSS_INCURRED_AMOUNT,
 15000.00 as PAID_ALLOCATED_LOSS_ADJUSTME_1,
 '020' as SCHEDULED_INDEMNITY_PERCENTA_0,
 'XXX01' as CARRIER_CODE,
CLAIM.ClaimNumber as CLAIM_NUMBER,
POLICY.PolicyNumber as POLICY_NUMBER_IDENTIFIER,
'001' as SEPARATED_SEGMENT_NUMBER,
'04' as EXPOSURE_STATE_CODE,
POLICY.EffectiveDate as POLICY_EFFECTIVE_DATE_ITEM,
'1' as REPORT_LEVEL_CODE_REPORT_NUM_0,
'0' as CORRECTION_SEQUENCE_NUMBER_C_0
FROM CC_CLAIM CLAIM 
JOIN cctl_jurisdiction jurisdiction on jurisdiction.ID = CLAIM.JurisdictionState
JOIN CC_POLICY POLICY ON POLICY.ID = CLAIM.POLICYID
JOIN CC_CLASSCODE CLASSCODE ON CLASSCODE.PolicyID = POLICY.ID
JOIN CC_EMPLOYMENTDATA EMPLOYMENTDATA ON EMPLOYMENTDATA.CLAIMID = CLAIM.ID
JOIN CC_INCIDENT INCIDENT ON INCIDENT.CLAIMID=  CLAIM.ID
JOIN CC_EXPOSURE EXPOSURE ON EXPOSURE.ClaimID = CLAIM.ID
JOIN cc_exposurerpt exposureReport on exposureReport.ExposureID = EXPOSURE.ID
JOIN cc_transaction transactions on transactions.ExposureID = EXPOSURE.ID
JOIN CC_CLAIMCONTACT CLAIMCONTACT ON CLAIMCONTACT.CLAIMID = CLAIM.ID  
JOIN CC_CLAIMCONTACTROLE CONTACTROLE ON CONTACTROLE.CLAIMCONTACTID = CLAIMCONTACT.ID 
JOIN CC_CONTACT CONTACT ON CONTACT.ID  = CLAIMCONTACT.CONTACTID) loss
join (Select 
CLAIM.ID as LINKID,
CLAIM.ClaimNumber as CLAIM_NUMBER,
'XXX01' as CARRIER_CODE,
POLICY.EffectiveDate as POLICY_EFFECTIVE_DATE_ITEM,
POLICY.PolicyNumber as POLICY_NUMBER_IDENTIFIER,
'001' as SEPARATED_SEGMENT_NUMBER,
'04' as EXPOSURE_STATE_CODE,
'1' as REPORT_LEVEL_CODE_REPORT_NUM_0,
'0' as CORRECTION_SEQUENCE_NUMBER_C_0
FROM cc_claim CLAIM 
JOIN CC_POLICY POLICY ON policy.ID = CLAIM.PolicyID) link
on link.CARRIER_CODE = loss.CARRIER_CODE and link.CLAIM_NUMBER = loss.CLAIM_NUMBER
and link.EXPOSURE_STATE_CODE = loss.EXPOSURE_STATE_CODE and link.CORRECTION_SEQUENCE_NUMBER_C_0 = loss.CORRECTION_SEQUENCE_NUMBER_C_0
and link.POLICY_EFFECTIVE_DATE_ITEM = loss.POLICY_EFFECTIVE_DATE_ITEM and link.POLICY_NUMBER_IDENTIFIER = loss.POLICY_NUMBER_IDENTIFIER
and link.REPORT_LEVEL_CODE_REPORT_NUM_0 = loss.REPORT_LEVEL_CODE_REPORT_NUM_0 and link.SEPARATED_SEGMENT_NUMBER = loss.SEPARATED_SEGMENT_NUMBER
</sql>
